{
    "manifest_version": 3,
    "name": "RealOemPlus",
    "description": "Enhance your parts diagram experience",
    "version": "1.0",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://www.realoem.com/bmw/enUS/showparts?id=*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "settings.html"
    }
  }
  